    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "uuid": "^8.3.0",
    "vue": "^2.6.12",
    "vue-router": "^3.4.3",
    "vueify": "^9.4.1",
    "watchify": "^3.11.1"
  }
}
